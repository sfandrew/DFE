<div align="center">
	<h1 id="main-header"><span class='text-danger'>Editing </span><%= @dynamic_form_entry.dynamic_form_type.name %></h1>
	<p><%= @dynamic_form_entry.dynamic_form_type.description %></p>
</div>

<% if @dynamic_form_type.form_type == "Multi-step" %> 
	<%= render 'multistep_form' %>
	<%= render 'field_group' %>

<% else %>
	<%= render 'default_form' %>
<% end %>


