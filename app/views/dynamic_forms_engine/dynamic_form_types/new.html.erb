<%= form_for(@dynamic_form_type, :html => {:class => "form-horizontal"}) do |f| %>
	<h3>Is a default form or Multi-step form</h3>
	<div class="col-sm-2">
		<%= f.select :form_type,['Default form','Multi-step'],{}, {:class => 'form-control input-medium form-type'} %>
	</div>
	<div class="default-form">
		<h1>New Form</h1>
		<%= render 'default_form' %>
	</div>

<% end %>

